{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "off"
        ],
        "no-console": [
            "off"
        ],
        "indent": [
            "error",
            4
        ],
        "no-var": [
            "off"
        ],
        "no-unused-vars": [
            "warn",
            {
                "args": "none"
            }
        ],
        "prefer-const": [
            "off"
        ],
        "no-plusplus": [
            "off"
        ],
        "no-param-reassign": [
            "off"
        ],
        "no-loop-func": [
            "off"
        ],
        "no-use-before-define": [
            "off"
        ],
        "no-alert": [
            "off"
        ],
        "radix": [
            "off"
        ]
    }
}